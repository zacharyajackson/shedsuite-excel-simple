{
  "name": "shedsuite-excel-integration",
  "version": "2.0.0",
  "description": "ShedSuite API to Microsoft Excel integration service with real-time sync capabilities",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon --no-colors --ignore logs/ src/index.js | cat",
    "test": "NODE_ENV=test jest",
    "test:unit": "NODE_ENV=test jest --testMatch='**/tests/unit/**/*.test.js'",
    "test:integration": "NODE_ENV=test jest --testMatch='**/tests/integration/**/*.test.js'",
    "test:e2e": "NODE_ENV=test jest --testMatch='**/tests/e2e/**/*.test.js'",
    "test:performance": "NODE_ENV=test jest --testMatch='**/tests/performance/**/*.test.js'",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:ci": "NODE_ENV=test jest --ci --coverage --watchAll=false",
    "test:production": "NODE_ENV=test jest --testMatch='**/tests/integration/production-failure-scenarios.test.js' --testMatch='**/tests/integration/chaos-testing.test.js' --testMatch='**/tests/e2e/sync-workflow-with-errors.test.js' --testMatch='**/tests/integration/regression-tests.test.js'",
    "test:load": "NODE_ENV=test jest --testMatch='**/tests/performance/concurrent-operations.test.js' --testMatch='**/tests/performance/adaptive-batch-performance.test.js'",
    "test:comprehensive": "NODE_ENV=test jest --testMatch='**/tests/integration/production-failure-scenarios.test.js' --testMatch='**/tests/integration/chaos-testing.test.js' --testMatch='**/tests/e2e/sync-workflow-with-errors.test.js' --testMatch='**/tests/integration/regression-tests.test.js' --testMatch='**/tests/performance/concurrent-operations.test.js' --testMatch='**/tests/performance/adaptive-batch-performance.test.js'",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "build": "echo \"No build step required for Node.js\" && exit 0",
    "health-check": "curl -f http://localhost:${PORT:-3000}/health || exit 1",
    "health-check:detailed": "curl -f http://localhost:${PORT:-3000}/health/detailed || exit 1",
    "railway:deploy": "chmod +x ./deploy-railway.sh && ./deploy-railway.sh",
    "deploy": "chmod +x ./scripts/deploy.sh && ./scripts/deploy.sh",
    "demo:improvements": "node development/demos/demo-improvements.js",
    "maintenance:enhanced": "node production/maintenance/enhanced-robust-maintenance.js",
    "maintenance:check": "node production/maintenance/pre-maintenance-check.js",
    "test:data": "node development/testing/verify-spreadsheet-data.js",
    "test:reliability": "NODE_ENV=test jest --testMatch='**/tests/integration/error-handling.test.js' --testMatch='**/tests/integration/monitoring-system.test.js'",
    "validate:config": "node -e \"require('dotenv').config(); const requiredVars = ['API_TOKEN', 'AZURE_CLIENT_ID', 'AZURE_TENANT_ID', 'AZURE_CLIENT_SECRET', 'EXCEL_WORKBOOK_ID']; const missing = requiredVars.filter(v => !process.env[v]); if (missing.length) { console.error('Missing required env vars:', missing); process.exit(1); } else { console.log('All required env vars present'); }\"",
    "monitor:health": "watch -n 10 'curl -s http://localhost:${PORT:-3000}/health | jq'",
    "monitor:metrics": "watch -n 10 'curl -s http://localhost:${PORT:-3000}/health/metrics | jq'"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "isomorphic-fetch": "^3.0.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.9",
    "open": "^10.1.2",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nock": "^13.4.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "shedsuite",
    "excel",
    "microsoft-graph",
    "api-integration",
    "real-time-sync"
  ],
  "author": "ShedSuite Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/shedsuite-excel-integration.git"
  }
}
