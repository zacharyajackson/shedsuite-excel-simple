# Server
PORT=8080
LOG_LEVEL=info

# ShedSuite API
SHEDSUITE_BASE_URL=https://app.shedsuite.com
SHEDSUITE_PUBLIC_API_PATH=/api/public
SHEDSUITE_TOKEN=
SHEDSUITE_ACCOUNT_ID=
INVENTORY_LIST_PATH=/api/public/inventory/v1
INVENTORY_PAGE_SIZE=100
INVENTORY_UPDATED_SINCE=

# Power BI (Service Principal)
POWERBI_TENANT_ID=
POWERBI_CLIENT_ID=
POWERBI_CLIENT_SECRET=
POWERBI_SCOPE=https://analysis.windows.net/powerbi/api/.default
POWERBI_GROUP_ID=
POWERBI_DATASET_ID=
POWERBI_DATASET_NAME=ShedSuite_Inventory
POWERBI_TABLE_NAME=InventoryItems
POWERBI_BATCH_SIZE=5000

# State store
STATE_STORE=supabase # memory | postgres | supabase
DATABASE_URL=

# Supabase Configuration
# SUPABASE_URL is required when using Supabase (instead of DATABASE_URL)
# SUPABASE_SERVICE_ROLE_KEY is required for Supabase authentication
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
SUPABASE_ANON_KEY=your_anon_key_here  # Optional, for public access if needed

# Security (optional)
SYNC_SHARED_SECRET=


